void drawBitmap(int16_t x, int16_t y, const uint8_t *bitmap, int16_t w, int16_t h, uint16_t color) {
  uint16_t ix, iy, byteWidth = w / 8;
  uint8_t b = 0;
  for (iy = 0; iy < h; iy++) {
    for (ix = 0; ix < w; ix+=8) {
      b = pgm_read_byte(bitmap + (( iy * w) + ix) / 8);
      if (b & 0x80) M5.Lcd.drawPixel(x+ix, y+iy, color);
      if (b & 0x40) M5.Lcd.drawPixel(x+ix+1 , y+iy, color);
      if (b & 0x20) M5.Lcd.drawPixel(x+ix+2, y+iy, color);
      if (b & 0x10) M5.Lcd.drawPixel(x+ix+3, y+iy, color);
      if (b & 0x08) M5.Lcd.drawPixel(x+ix+4, y+iy, color);
      if (b & 0x04) M5.Lcd.drawPixel(x+ix+5, y+iy, color);
      if (b & 0x02) M5.Lcd.drawPixel(x+ix+6, y+iy, color);
      if (b & 0x01) M5.Lcd.drawPixel(x+ix+7, y+iy, color);

    }
  }
}

void drawBitmap2(int16_t x, int16_t y, const uint8_t *bitmap, int16_t w, int16_t h, uint16_t color) {
  uint16_t c, ix, iy, byteWidth = w / 8;
  uint8_t b = 0;
  for (iy = 0; iy < h; iy++) {
    for (ix = 0; ix < w; ix+=8) {
      b = pgm_read_byte(bitmap + (( iy * w) + ix) / 8);
      for (int i = 0; i < 8; i++) {
        c = BLACK;
        if (b & (0x80 >> i)) c = color;
        M5.Lcd.drawPixel(x+ix+i, y+iy, c);
      }
    }
  }
}



